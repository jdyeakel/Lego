pages = {6388--6396},
month = jun,
publisher = {National Acad Sciences},
affiliation = {School of Archaeology, University of Oxford, Oxford OX1 2PG, United Kingdom; Max Planck Institute for the Science of Human History, Jena D-07743, Germany; boivin@shh.mpg.de.},
doi = {10.1073/pnas.1525200113},
