}


@article{Haerter2016,
author = {Haerter, Jan O and Mitarai, Namiko and Sneppen, Kim},
title = {{Food Web Assembly Rules for Generalized Lotka-Volterra Equations}},
journal = {PLoS Comp. Biol.},
year = {2016},
volume = {12},
number = {2},
pages = {e1004727},
month = feb,
publisher = {Public Library of Science},
doi = {10.1371/journal.pcbi.1004727},
language = {English},
read = {Yes},
rating = {0},
date-added = {2017-07-21T22:28:51GMT},
date-modified = {2018-03-09T22:04:28GMT},
abstract = {Author Summary Human impact currently induces rapid reductions in global biodiversity. Assessing the consequences of such modifications requires that ecological science better understand the conditions under which the species in a community can coexist and when not. Fundamentally, two species can not coexist indefinitely when they exclusively compete for the same prey{\textemdash}one must inevitably become extinct. This paradigm is known as the competitive exclusion principle. We consider communities of any number of species and multiple trophic levels, i.e. the average number of steps between a predator and basic nutrient, e.g. sunlight or sugars. We show that the extension of the competitive exclusion principle to such large systems means that each species must be part of a {\textquotedblleft}non-overlapping pairing{\textquotedblright}. Such pairings are exclusive connections between two species, e.g. a predator and a prey. We demonstrate that a stable food web can always be obtained if a non-overlapping pairing exists. The food web assembly rules are explicit conditions that specify sustainable combinations of species at the different trophic levels. As also seen in field data, our rules imply high species numbers at intermediate levels and few at the top and bottom. We further show that omnivorous species{\textemdash}those with hosts at multiple trophic levels{\textemdash}may take a special role in stabilizing food webs, as they combine several trophic levels.},
url = {http://dx.plos.org/10.1371/journal.pcbi.1004727},
local-url = {file://localhost/Users/justinyeakel/Dropbox/Library.papers3/Files/69/69493803-DA0C-465D-8DEC-D89DB56F4FFA.pdf},
file = {{69493803-DA0C-465D-8DEC-D89DB56F4FFA.pdf:/Users/justinyeakel/Dropbox/Library.papers3/Files/69/69493803-DA0C-465D-8DEC-D89DB56F4FFA.pdf:application/pdf;69493803-DA0C-465D-8DEC-D89DB56F4FFA.pdf:/Users/justinyeakel/Dropbox/Library.papers3/Files/69/69493803-DA0C-465D-8DEC-D89DB56F4FFA.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1371/journal.pcbi.1004727}}
}


