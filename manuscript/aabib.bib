@article{Yeakel2014,
author = {Yeakel, Justin D and Pires, M M and Rudolf, L and Dominy, N J and Koch, P L and Guimar{\~a}es Jr, Paulo R and Gross, T},
title = {{Collapse of an ecological network in Ancient Egypt}},
journal = {Proceedings of the National Academy of Sciences},
year = {2014},
volume = {111},
number = {40},
pages = {14472--14477},
month = sep,
doi = {10.1073/pnas.1408471111},
language = {English},
read = {Yes},
rating = {0},
date-added = {2014-09-08T20:12:50GMT},
date-modified = {2018-02-26T00:26:32GMT},
abstract = {The dynamics of ecosystem collapse are fundamental to determining how and why biological communities change through time, as well as the potential effects of extinctions on ecosystems. Here, we integrate depictions of mammals from Egyptian antiquity with direct lines of paleontological and archeological evidence to infer local extinctions and community dynamics over a 6,000-y span. The unprecedented temporal resolution of this dataset enables examination of how the tandem effects of human population growth and climate change can disrupt mammalian communities. We show that the extinctions of mammals in Egypt were nonrandom and that destabilizing changes in community composition coincided with abrupt aridification events and the attendant collapses of some complex societies. We also show that the roles of species in a community can change over time and that persistence is predicted by measures of species sensitivity, a function of local dynamic stability. To our knowledge, our study is the first high-resolution analysis of the ecological impacts of environmental change on predator{\textendash}prey networks over millennial timescales and sheds light on the historical events that have shaped modern animal communities.},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1408471111},
local-url = {file://localhost/Users/justinyeakel/Dropbox/Library.papers3/Files/0D/0D843835-D765-4975-B29C-57832AC553DD.pdf},
file = {{0D843835-D765-4975-B29C-57832AC553DD.pdf:/Users/justinyeakel/Dropbox/Library.papers3/Files/0D/0D843835-D765-4975-B29C-57832AC553DD.pdf:application/pdf;0D843835-D765-4975-B29C-57832AC553DD.pdf:/Users/justinyeakel/Dropbox/Library.papers3/Files/0D/0D843835-D765-4975-B29C-57832AC553DD.pdf:application/pdf}},
